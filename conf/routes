# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /zeke                      controllers.Application.zeke()
GET     /nikki                      controllers.Application.nikki()
GET     /kyuss                      controllers.Application.kyuss()
GET     /cjhobgood                      controllers.Application.cjhobgood()
GET     /kekai                      controllers.Application.kekai()
GET     /kelly                      controllers.Application.kelly()
GET     /bethany                    controllers.Application.bethany()
GET     /jake                       controllers.Application.jake()
GET     /sunny                      controllers.Application.sunny()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
